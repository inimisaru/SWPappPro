@model IEnumerable<SWPappPro.Models.GABINET>
@{
    ViewBag.Title = "UmowWizyteKonWybierzGabinet";
    Layout = "~/Views/Shared/_LekarzLayout.cshtml";
}
<br />
<h3>Wybierz gabinet:</h3>
<br />
<table class="table">
    <tr>
        <th>
            Numer gabinetu
        </th>
        <th>
            Numer budynku
        </th>
        <th>
            Rodzaj gabinetu
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NR)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BUDYNEK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RODZAJ)
            </td>
            <td>
                @Html.ActionLink("Wybierz", "UmowWizyteKonWybierzPacjenta", new { gabinet_id = item.GABINET_ID })
            </td>
        </tr>
    }

</table>