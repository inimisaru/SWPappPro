
@{
    ViewBag.Title = "PrzeplanujWizyteFormularz";
    Layout = "~/Views/Shared/_PacjentLayout.cshtml";
}

@model SWPappPro.Models.WIZYTA_DOMOWA
<br />
<h3>Potwierdz przeplanowanie wizyty:</h3>
<br />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.WIZYTA_DOMOWA_ID)


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Zatwierdz" class="btn btn-default" />
            </div>
        </div>
    </div>
}
